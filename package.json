{
  "name": "yaar",
  "version": "1.4.14",
  "description": "Yet Another API Router",
  "main": "./dist/lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/zipscene/yaar.git"
  },
  "keywords": [
    "api",
    "router"
  ],
  "scripts": {
    "lint": "node ./node_modules/.bin/eslint . || exit 0",
    "docs": "node ./node_modules/.bin/yuidoc .",
    "test": "npm run-script build && node ./node_modules/.bin/mocha ./dist/test -R spec",
    "build": "rm -rf dist && ./node_modules/.bin/broccoli build dist && rm -rf tmp",
    "prepublish": "npm run-script build && npm run-script docs"
  },
  "dependencies": {
    "babel-runtime": "5.8.20",
    "body-parser": "^1.13.3",
    "common-schema": "^1.8.0",
    "cors": "^2.7.1",
    "crisphooks": "^2.0.0",
    "express": "^4.13.3",
    "lodash": "^3.10.1",
    "objtools": "^1.6.2",
    "pasync": "^1.4.0",
    "request": "^2.69.0",
    "xerror": "^1.0.0",
    "zstreams": "^3.2.2"
  },
  "devDependencies": {
    "broccoli": "^0.16.7",
    "broccoli-babel-transpiler": "^5.4.5",
    "broccoli-cli": "^1.0.0",
    "broccoli-merge-trees": "^0.2.3",
    "broccoli-static-compiler": "^0.2.1",
    "broccoli-wrap": "0.0.2",
    "chai": "^3.2.0",
    "es6-promise": "^3.0.2",
    "eslint": "^1.2.1",
    "mocha": "^2.2.5",
    "request": "^2.81.0",
    "sinon": "^2.1.0",
    "sinon-chai": "^2.9.0",
    "source-map-support": "^0.3.2",
    "supertest": "^1.1.0",
    "yuidocjs": "^0.9.0"
  },
  "license": "Apache-2.0"
}
