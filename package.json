    "prepublish": "npm run-script build && npm run-script docs"
  },
  "dependencies": {
    "babel-runtime": "5.8.20",
    "body-parser": "^1.13.3",
    "cors": "^2.7.1",
    "express": "^4.13.3",
    "lodash": "^3.10.1",
    "pasync": "^1.4.0",
    "xerror": "^1.0.0"
  },
  "devDependencies": {
    "broccoli": "^0.16.7",
    "broccoli-babel-transpiler": "^5.4.5",
    "broccoli-cli": "^1.0.0",
    "broccoli-merge-trees": "^0.2.3",
    "broccoli-static-compiler": "^0.2.1",
    "broccoli-wrap": "0.0.2",
    "chai": "^3.2.0",
    "es6-promise": "^3.0.2",
    "eslint": "^1.2.1",
    "mocha": "^2.2.5",
    "source-map-support": "^0.3.2",
    "supertest": "^1.1.0",
    "yuidocjs": "^0.9.0"
  },
  "publishConfig": {
    "registry": "https://npm.zipscene.com/"
